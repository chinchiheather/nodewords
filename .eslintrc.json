{
    "extends": "airbnb-base",

    "rules": {
        "class-methods-use-this": 0,
        "comma-dangle": ["error", "never"],
        "max-len": ["error", 120],
        "no-console": 0,
        "no-mixed-operators": 0,
        "no-plusplus": 0,
        "no-use-before-define": ["error", { "functions": false }]
    }
}